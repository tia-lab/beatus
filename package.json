{
  "name": "nextjs-starter-kit",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "redirects": "node scripts/redirects/index.mjs",
    "types": "bun ./scripts/sanity/types.mjs",
    "lint": "bun ./scripts/lint/index.mjs",
    "get-env": "node ./scripts/env/load-env.mjs",
    "beatus": "npm run get-env beatus",
    "hermitage": "npm run get-env hermitage",
    "css": "tsx ./scripts/scss/index.mjs && bun ./scripts/scss/lint/index.mjs",
    "dev": "bun run css && next dev -p 9852",
    "build": "bun run css && next build",
    "postbuild": "next-sitemap --config ./scripts/sitemap/index.js",
    "build-commit": "next build",
    "start": "next start -p 9852",
    "schema": "dotenv -c -- bash -c 'gql.tada generate schema https://graphql.datocms.com --header \"X-Exclude-Invalid: true\" --header \"Authorization: $DATOCMS_PUBLISHED_CONTENT_CDA_TOKEN\"'",
    "format": "npx prettier . --write",
    "prepare": "husky && npx simple-git-hooks && bun schema",
    "pull-env": "vercel env pull",
    "dato-clone": "NODE_OPTIONS=\"--loader ts-node/esm\" ts-node scripts/dato/clone.ts"
  },
  "dependencies": {
    "@datocms/cda-client": "^0.2.1",
    "@datocms/cma-client": "^3.3.5",
    "@formspree/react": "^2.5.1",
    "@google-recaptcha/react": "^1.2.3",
    "@react-google-maps/api": "^2.20.3",
    "@studio-freight/react-lenis": "^0.0.43",
    "@types/highlight.js": "^10.1.0",
    "clsx": "^2.1.1",
    "datocms-structured-text-to-plain-text": "^4.0.1",
    "datocms-structured-text-utils": "^4.0.1",
    "embla-carousel-react": "^8.5.1",
    "formik": "^2.4.6",
    "gql.tada": "^1.8.0",
    "gsap": "npm:@gsap/business@^3.12.5",
    "highlight.js": "^11.10.0",
    "html-react-parser": "^5.2.2",
    "jsdom": "^24.1.0",
    "lucide-react": "^0.475.0",
    "next": "14.2.4",
    "next-intl": "^3.24.0",
    "next-sitemap": "^4.2.3",
    "next-video": "^1.4.0",
    "react": "^18",
    "react-aria-components": "^1.4.1",
    "react-datocms": "^7.0.1",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18",
    "react-player": "^2.16.0",
    "readline-sync": "^1.4.10",
    "serialize-error": "^11.0.3",
    "tsx": "^4.19.2",
    "usehooks-ts": "^3.1.0",
    "zustand": "^4.0.0"
  },
  "devDependencies": {
    "@0no-co/graphqlsp": "^1.12.10",
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@iconify-icon/react": "^2.1.0",
    "@svgr/webpack": "^8.1.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/readline-sync": "^1.4.8",
    "boxen": "^8.0.1",
    "chalk": "^5.3.0",
    "commitlint": "^19.4.0",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.4",
    "prettier": "3.3.3",
    "sass": "^1.77.8",
    "simple-git-hooks": "^2.11.1",
    "stylelint": "^16.8.2",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-prettier": "^5.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "simple-git-hooks": {
    "pre-commit": "npm run format"
  }
}
